{
  "name": "solidity-shared-lib",
  "version": "0.2.1",
  "description": "Specifies contracts shared between libraries",
  "main": "truffle.js",
  "scripts": {
    "test:all": "./node_modules/truffle/build/cli.bundled.js test --network test",
    "run:rpc": "./node_modules/ganache-cli/build/cli.node.js -g 1 --gasLimit 4700000 --trace_request true | grep -Ev \"FilterSubprovider|eth_getFilterChanges\"",
    "compile": "./node_modules/truffle/build/cli.bundled.js compile",
    "compile:all": "./node_modules/truffle/build/cli.bundled.js compile --all",
    "migrate": "./node_modules/truffle/build/cli.bundled.js migrate",
    "migrate:reset": "./node_modules/truffle/build/cli.bundled.js migrate --reset",
    "console": "./node_modules/truffle/build/cli.bundled.js console --network test"
  },
  "devDependencies": {
    "ganache-cli": "^6.1.4",
    "truffle": "^4.1.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChronoBank/solidity-shared-lib.git"
  },
  "author": "LaborX PTY",
  "bugs": {
    "url": "https://github.com/ChronoBank/solidity-shared-lib/issues"
  },
  "homepage": "https://github.com/ChronoBank/solidity-shared-lib#readme",
  "license": "AGPL-3.0"
}
